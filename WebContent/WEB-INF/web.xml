<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>JstlMaven</display-name>

	<servlet>
		<servlet-name>ListCittaServlet</servlet-name>
		<servlet-class>it.objectmethod.servlet.ListCittaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListCittaServlet</servlet-name>
		<url-pattern>/ListCittaServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListContinent</servlet-name>
		<servlet-class>it.objectmethod.servlet.ListContinentServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListContinent</servlet-name>
		<url-pattern>/ListContinent</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListNazioni</servlet-name>
		<servlet-class>it.objectmethod.servlet.ListNazioniServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListNazioni</servlet-name>
		<url-pattern>/ListNazioni</url-pattern>
	</servlet-mapping>

</web-app>